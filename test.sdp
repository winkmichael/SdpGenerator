
struct Vec3{
    float X = 1;
    float Y = 2;
    float Z = 3;
}

struct Transform{
    Vec3 Pos = 1;
    Vec3 Forward = 2;
    Vec3 Sacle = 3;
}

struct Mesh{
    List<Vec3> Vesters = 1;
    List<int> Triangle = 2;
    Transform  Trans = 3;
}

struct Box{
    Vec3 Size = 1;
}

struct Scene{
    List<Mesh> Meshs = 1;
    List<Box> Boxs = 2;
    Map<int, int> MapTest = 3;
}